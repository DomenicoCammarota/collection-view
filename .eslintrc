{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-var": 2,
    "no-const-assign": 2,
    "prefer-const": 2,
    "no-param-reassign": [2, {"props": false}],
    "no-unused-vars": [2, {"vars": "local", "argsIgnorePattern": "^_"}],
    "no-plusplus": 2,
    "operator-assignment": [2, "always"],
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 2,
    "one-var": [2, "never"],
    "prefer-arrow-callback": 2,
    "no-use-before-define": 2,
    "no-constant-condition": 2,
    "no-cond-assign": 2,
    "consistent-return": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "no-extra-semi": 2,
    "no-empty": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eqeqeq": 2,
    "no-dupe-class-members": 2,
    "arrow-spacing": 2,
    "no-arrow-condition": 2,
    "no-class-assign": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-alert": 2,
    "linebreak-style": [2, "unix"],
    "max-len": [2, 100, 2, {"ignoreUrls": true}],
    "no-undef": 2,
    "no-shadow": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-unreachable": 2,
    "no-else-return": 2,
    "semi": [2, "never"],
    "guard-for-in": 2,
    "no-caller": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "yoda": 2,
    "indent": [2, 2, {"SwitchCase": 2}],
    "comma-style": [2, "last"],
    "eol-last": 2,
    "no-extra-semi": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-floating-decimal": 2
  }
}
